#include <iostream>
#include <math.h>
#include <iomanip>
using namespace std;
int main()
{  
    float a, b, c, sum, d, e, f;
    cout<<"chuong trinh xap xep 3 so tu nhien tang dan. \n"<<"Xin nhap 3 so tu nhien: "<<endl;
    cin >> a >> b >> c;
    sum = a + b + c;
    d = min(min(a, b), c);
    f = max(max(a, b), c);
    e = sum - (d + f);
    cout << d << " " << e << " " << f;
    return 0;
}